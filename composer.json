{
  "name": "wedevelop/silverstripe-elastica",
  "description": "elasticsearch extension for silverstripe",
  "type": "silverstripe-vendormodule",
  "keywords": ["elastica", "elasticsearch", "facet", "filters"],
  "require": {
      "ext-json": "*",
      "php": ">=8.1",
      "silverstripe/framework": "^4.12",
      "ruflin/elastica": "^7.1",
      "symbiote/silverstripe-gridfieldextensions": "^3.4",
      "silverstripe/tagfield": "^2.9"
  },
  "autoload": {
    "psr-4": {
        "WeDevelop\\Elastica\\": "src/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4"
  }
}
